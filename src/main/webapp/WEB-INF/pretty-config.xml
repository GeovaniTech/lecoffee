<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/pages/login.xhtml" />
	</url-mapping>

	<url-mapping id="loginParam">
		<pattern value="/login/#{registerFinished}" />
		<view-id value="/pages/login.xhtml?registerFinished=#{registerFinished}" />
	</url-mapping>
	
	<url-mapping id="newPassword">
		<pattern value="/newpassword" />
		<view-id value="/pages/newPassword.xhtml" />
	</url-mapping>
	
	<url-mapping id="newPasswordParam">
		<pattern value="/newpassword/#{token}" />
		<view-id value="/pages/newPassword.xhtml?token=#{token}" />
	</url-mapping>
	
	<url-mapping id="register">
		<pattern value="/register" />
		<view-id value="/pages/register.xhtml" />
	</url-mapping>
	
	<url-mapping id="adminPedidos">
		<pattern value="/admin/pedidos" />
		<view-id value="/pages/admin/orders/pedidos.xhtml"/>
	</url-mapping>
	
	<url-mapping id="adminProdutos">
		<pattern value="/admin/produtos" />
		<view-id value="/pages/admin/products/products.xhtml"/>
	</url-mapping>	
	
	<url-mapping id="adminPayments">
		<pattern value="/admin/payments" />
		<view-id value="/pages/admin/payments/payments.xhtml"/>
	</url-mapping>	
	
	<url-mapping id="adminEarnings">
		<pattern value="/admin/earnings" />
		<view-id value="/pages/admin/earnings.xhtml"/>
	</url-mapping>	
	
	<url-mapping id="adminAvaliacoes">
		<pattern value="/admin/avaliacoes" />
		<view-id value="/pages/admin/avaliacoes.xhtml"/>
	</url-mapping>	

	<url-mapping id="adminUsers">
		<pattern value="/admin/users" />
		<view-id value="/pages/admin/users/users.xhtml"/>
	</url-mapping>	

	<url-mapping id="adminChats">
		<pattern value="/admin/chats" />
		<view-id value="/pages/admin/chats.xhtml"/>
	</url-mapping>	

	<url-mapping id="adminConfiguracoes">
		<pattern value="/admin/configuracoes" />
		<view-id value="/pages/admin/configuracoes.xhtml"/>
	</url-mapping>	

	<url-mapping id="home">
		<pattern value="/home" />
		<view-id value="/pages/client/landing-page.xhtml" />
	</url-mapping>
	
	<url-mapping id="myOrders">
		<pattern value="/meuspedidos" />
		<view-id value="/pages/client/pedidos.xhtml" />
	</url-mapping>
</pretty-config>